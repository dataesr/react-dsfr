  "license": "MIT",
  "repository": "dataesr/react-dsfr",
  "main": "dist/index.min.cjs.js",
  "source": "src/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "react",
    "components",
    "design-system",
    "gouvfr"
  ],
  "engines": {
    "node": ">=14.18",
